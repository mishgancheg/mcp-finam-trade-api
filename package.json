{
  "name": "mcp-finam-trade-api",
  "version": "1.0.33",
  "description": "Model Context Protocol (MCP) server for FINAM Trade API - provides trading tools for Claude AI and other MCP clients",
  "main": "dist/src/mcp/index.js",
  "type": "module",
  "bin": {
    "mcp-finam-trade-api": "dist/src/mcp/index.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "mcp": "node dist/src/mcp/index.js",
    "mcp:http": "node dist/src/mcp/index.js --http",
    "start": "node dist/src/mcp/index.js",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist",
    "cb": "npm run clean && npm run build",
    "dev": "tsx watch src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "test:api": "node test/tester-api.js",
    "test:endpoints": "node test/tester-endpoints.js",
    "emulator": "node test/emulator/emulator.js",
    "emulator:check": "node test/emulator/check-emulator.js",
    "emulator:kill": "node scripts/kill-emulator.js",
    "kill:port": "node scripts/kill-port.js",
    "kill:emulator": "node scripts/kill-emulator.js",
    "test:mcp:http": "node test/mcp/http.js",
    "test:mcp:sse": "node test/mcp/sse.js",
    "test:mcp:stdio": "node test/mcp/stdio.js",
    "test:search:direct": "node test/search/direct-search.js",
    "test:search:http": "node test/search/test-search-http.js",
    "test:search:stdio": "node test/search/test-search-stdio.js",
    "version:patch": "npm version patch -m \"chore: bump version to %s\"",
    "version:minor": "npm version minor -m \"chore: bump version to %s\"",
    "version:major": "npm version major -m \"chore: bump version to %s\"",
    "release:patch": "npm run version:patch && git push && git push --tags",
    "release:minor": "npm run version:minor && git push && git push --tags",
    "release:major": "npm run version:major && git push && git push --tags"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "FINAM",
    "trade-api",
    "trade",
    "api",
    "ai",
    "automation"
  ],
  "author": {
    "name": "Michael Makarov",
    "email": "github@mmak.site"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mishgancheg/mcp-finam-trade-api.git"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.2",
    "chalk": "^5.6.2",
    "config": "^4.1.1",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "got": "^14.4.9",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/config": "^3.3.5",
    "@types/express": "^5.0.3",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "eslint-config-at-25": "^25.9.6",
    "eslint-plugin-import": "^2.32.0",
    "eventsource": "^2.0.2",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
}
